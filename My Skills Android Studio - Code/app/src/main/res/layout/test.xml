<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/vis"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical">
		<LinearLayout
			android:id="@+id/linear3"
			android:layout_width="match_parent"
			android:layout_height="55dp"
			android:padding="8dp"
			android:background="#3500A5"
			android:gravity="center_horizontal|bottom"
			android:orientation="horizontal">
			<TextView
				android:id="@+id/textview1"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:text="My Skills (Test Section)"
				android:textSize="20sp"
				android:textStyle="bold"
				android:textColor="#FFFFFF" />
		</LinearLayout>
		<ListView
			android:id="@+id/listview1"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:background="@android:color/transparent"
			android:choiceMode="none" />
	</LinearLayout>
	<LinearLayout
		android:id="@+id/shim"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical">
		<LinearLayout
			android:id="@+id/linear4"
			android:layout_width="match_parent"
			android:layout_height="55dp"
			android:padding="8dp"
			android:background="#3500A5"
			android:gravity="center_horizontal|bottom"
			android:orientation="horizontal">
			<TextView
				android:id="@+id/textview2"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:text="My Skills (Test Section)"
				android:textSize="20sp"
				android:textStyle="bold"
				android:textColor="#FFFFFF" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/backs4ppp"
			android:layout_width="match_parent"
			android:layout_height="100dp"
			android:layout_margin="5dp"
			android:padding="2dp"
			android:background="#FFFFFF"
			android:orientation="horizontal">
			<com.facebook.shimmer.ShimmerFrameLayout
				android:id="@+id/s1"
				android:layout_width="100dp"
				android:layout_height="match_parent"
				android:layout_margin="5dp"
				android:background="#E0E0E0"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="vertical" />
			<LinearLayout
				android:id="@+id/linear5"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:padding="8dp"
				android:background="#FFFFFF"
				android:gravity="center_vertical"
				android:orientation="vertical"
				android:layout_weight="1">
				<com.facebook.shimmer.ShimmerFrameLayout
					android:id="@+id/s2"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:background="#E0E0E0"
					android:orientation="horizontal" />
				<com.facebook.shimmer.ShimmerFrameLayout
					android:id="@+id/s3"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="5dp"
					android:padding="8dp"
					android:background="#E0E0E0"
					android:orientation="horizontal" />
			</LinearLayout>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear23sppp"
			android:layout_width="match_parent"
			android:layout_height="100dp"
			android:layout_margin="5dp"
			android:padding="2dp"
			android:background="#FFFFFF"
			android:orientation="horizontal">
			<com.facebook.shimmer.ShimmerFrameLayout
				android:id="@+id/linear24s"
				android:layout_width="100dp"
				android:layout_height="match_parent"
				android:layout_margin="5dp"
				android:background="#E0E0E0"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="vertical" />
			<LinearLayout
				android:id="@+id/linear25"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:padding="8dp"
				android:background="#FFFFFF"
				android:gravity="center_vertical"
				android:orientation="vertical"
				android:layout_weight="1">
				<com.facebook.shimmer.ShimmerFrameLayout
					android:id="@+id/linear26s"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:background="#E0E0E0"
					android:orientation="horizontal" />
				<com.facebook.shimmer.ShimmerFrameLayout
					android:id="@+id/linear27s"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="5dp"
					android:padding="8dp"
					android:background="#E0E0E0"
					android:orientation="horizontal" />
			</LinearLayout>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear28pppppp"
			android:layout_width="match_parent"
			android:layout_height="100dp"
			android:layout_margin="5dp"
			android:padding="2dp"
			android:background="#FFFFFF"
			android:orientation="horizontal">
			<com.facebook.shimmer.ShimmerFrameLayout
				android:id="@+id/linear29s"
				android:layout_width="100dp"
				android:layout_height="match_parent"
				android:layout_margin="5dp"
				android:background="#E0E0E0"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="vertical" />
			<LinearLayout
				android:id="@+id/linear30"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:padding="8dp"
				android:background="#FFFFFF"
				android:gravity="center_vertical"
				android:orientation="vertical"
				android:layout_weight="1">
				<com.facebook.shimmer.ShimmerFrameLayout
					android:id="@+id/linear31s"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:background="#E0E0E0"
					android:orientation="horizontal" />
				<com.facebook.shimmer.ShimmerFrameLayout
					android:id="@+id/linear32s"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="5dp"
					android:padding="8dp"
					android:background="#E0E0E0"
					android:orientation="horizontal" />
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
